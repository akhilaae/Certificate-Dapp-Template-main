pragma solidity 0.8.18;

contract Cert{
    address admin;

    constructor() {
        admin = msg.sender;
    }

    struct certificate{
        string name;
        string course;
        string grade;
        string date;
    }
    mapping(uint256 => certificate) public certificates;

    function issue(
        uint256 _id,
        string memory _name,
        string memory _course,
        string memory _grade,
        string memory _date
    ) public{
        require(admin == msg.sender,"Not authorized");
        certificates[_id] = certificate(_name,_course,_grade,_date);
    }
}


